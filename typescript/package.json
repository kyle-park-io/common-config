{
  "scripts": {
    "commit-last": "commitlint --from HEAD~1 --to HEAD",
    "commit-lint": "commitlint --from main --to HEAD",
    "eslint-format": "eslint \"src/**/*.ts\" --fix",
    "eslint-lint": "eslint \"src/**/*.ts\"",
    "husky-postinstall": "husky install",
    "husky-postpack": "pinst --enable",
    "husky-prepack": "pinst --disable",
    "prettier-format": "prettier --write .",
    "prettier-lint": "prettier --check ."
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "pinst": "^3.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "*"
  }
}
